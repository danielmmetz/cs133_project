<tr>
  <td> <%= link_to "X", {:action => :destroy, group_id: group.id}, :method => :delete , :class => "btn btn-default" %></td>
  <td> <%= group.id %> </td>
  <td>
    <% students_in(group).each do |mem|-%>
      <%= mem.name %> <br/>
      <%= form_for :member do |f| -%>
        <%= f.text_field :student_id, placeholder: "student id" %>
        <%= f.submit "Add" %>
      <% end -%>
    <% end -%>
  </td>
  <td> <%= (Student.find group.student_id).name%> </td>
  <td> <%= group.draw_num %> </td>
  <td> <%= group.for_suite ? "YES" : "NO"%> </td>
</tr>